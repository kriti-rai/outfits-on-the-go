<br>
<h1> Post an Outfit </h1>
<br>
<%= form_for @outfit, url: board_outfits_path, method: :post do |f| %>
<div class="form-group">
  <%=f.file_field :image %>
</div>
  <%= f.text_field :caption, placeholder: "Say something..." %>
  <%= f.hidden_field :user_id %>
  <%= f.hidden_field :board_id %>

  <%= f.submit "Post"%>
<% end %>
